spring.application.name=OrderManagement
server.port = 8084
spring.jpa.properties.hibernate.dialect = org.hibernate.dialect.PostgressSQLDialect
spring.datasource.driver-class-name=org.postgressql.Driver
spring.datasource.url=jdbc:postgresql://localhost:5432/OrderMgmt
spring.datasource.username =postgres
